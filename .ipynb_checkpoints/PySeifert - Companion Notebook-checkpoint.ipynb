    "            lvals = list(set(list(itertools.product(range(-amax,amax),repeat=rk)))-set(list(itertools.product(range(-amax+1,amax-1),repeat=rk))))\n",
    "            exp = [1/(2*self.m)*NormL(self.m*(vector(l)+rho+lam)-CartanM(2).inverse()*vector(s)) for l in lvals]\n",
    "            overMax = np.greater_equal(exp,expMax)\n",
    "            if np.logical_and.reduce(overMax):\n",
    "                rejected+=1\n",
    "            else:\n",
    "                rejected = 0\n",
    "                if explicit:\n",
    "                    [chi.append(q^(e)*(lambda x: WeylCycle(x))(vector(l)+rho+lam)/WCr) for e,l in zip(np.array(exp,dtype=sage.rings.rational.Rational)[np.logical_not(overMax)].tolist(),np.array(lvals)[np.logical_not(overMax)].tolist())]\n",
    "                else:\n",
    "                    [chi.append(q^(e)*(lambda x: Delta(x[0],x[1]) if (x[0] != 0) and (x[1] != 0) else 0)(vector(l)+rho+lam)/WCr) for e,l in zip(np.array(exp,dtype=sage.rings.rational.Rational)[np.logical_not(overMax)].tolist(),np.array(lvals)[np.logical_not(overMax)].tolist())]\n",
    "            amax+=1\n",
    "        return sum(chi)\n",
    "    \n",
    "    def chiTilde(self,b,wh, expMax):\n",
