    "def CartanM(rk):\n",
    "    \"\"\"\n",
    "    Return the Cartan matrix for A_rk\n",
    "    \"\"\"\n",
    "    return matrix(rk,lambda x,y: 2 if x==y else (-1 if abs(x-y)==1 else 0))\n",
    "\n",
    "def AWeylGroup(n, WL = False, basis = \"root\"):\n",
    "    \"\"\"\n",
    "    Compute the Weyl group for dimensions 1 and 2. If WL flag is set to true return Weil Lengths also.\n",
    "    Improvements: make it more general for all A_n\n",
    "    \"\"\"\n",
    "    assert basis in [\"root\", \"weight\"], \"Basis must be root or weight\"\n",
    "    \n",
    "    if n == 1:\n",
    "        WG = [matrix([[1]]),matrix([[-1]])]\n",
    "        if WL:\n",
    "            return [WG,[0,1]]\n",
    "        else:\n",
    "            return WG\n",
    "    if n == 2:\n",
    "        A = matrix([[-1,1],[0,1]])\n",
    "        B = matrix([[1,0],[1,-1]])\n",
    "        if basis == \"weight\":\n",
    "            A=A.transpose()\n",
    "            B=B.transpose()\n",
    "        WG = [identity_matrix(2),A,B,A*B,B*A,A*B*A]\n",
    "        if WL:\n",
    "            return [WG,[0,1,1,0,0,1]]\n",
    "        else:\n",
    "            return WG\n",
    "    \n",
    "def NormL(v1,v2 = None, basis = \"root\"):\n",
    "    \"\"\"\n",
    "    Calculate the product between the A_n lattice vectors v1, v2\n",
    "    \"\"\"\n",
    "    assert basis in [\"root\", \"weight\"], \"Basis must be root or weight\"\n",
    "    \n",
    "    if v2 == None:\n",
    "        v2 = v1\n",
    "    assert len(v1)==len(v2), \"Vectors must have same dimension\"\n",
    "    \n",
    "    if basis == \"root\":\n",
    "        mat = CartanM(len(v1))\n",
    "    elif basis == \"weight\":\n",
    "        mat = CartanM(len(v1)).inverse()\n",
    "    \n",
    "    return vector(v1)*mat*vector(v2)\n",
    "\n",
    "def WeylCycle(v,z = None, dim = None):\n",
    "    \"\"\"\n",
    "        Compute z0,z1,z2 with powers given by cycling v with the Weyl group.\n",
    "        Signs are given by the Weyl length.\n",
    "    \"\"\"\n",
    "    if dim == None: dim = len(v)\n",
    "        \n",
    "    if z==None:\n",
    "        varstr = \"\"\n",
    "        for i in range(dim):\n",
    "            varstr +=f\", z{i}\"\n",
    "        z = var(varstr)\n",
    "    else:\n",
    "        assert len(z) == dim, \"Dimensions of the vector and variables do not match\"\n",
    "        \n",
    "    WG,WL = AWeylGroup(dim,WL=True)\n",
    "    WGv = list()\n",
    "    for g,l in zip(WG,WL):\n",
    "        WGv.append([g*vector(v),l])\n",
    "    cycle = list()\n",
    "    for gv in WGv:\n",
    "        cycle.append((-1)^(gv[1])*product([x**y for x,y in zip(z,gv[0])]))\n",
    "    return sum(cycle)\n",
    "\n",
    "def par(a,b,s,m,expMax,q = var(\"q\")):\n",
    "    \"\"\"\n",
    "    Computes Sung's parenthesis function between a,b.\n",
    "    \"\"\"\n",
    "    rejected = 0\n",
    "    amax = 1\n",
    "    par = list()\n",
    "    s1, s2 = s\n",
    "    while rejected < 3:\n",
    "        mvals = list(set(list(itertools.product(range(amax),repeat=3)))-set(list(itertools.product(range(amax-1),repeat=3))))\n",
    "        exp = [m*(m1**2 + m2**2+m3**2-m1*m2+m2*m3+m1*m3)+a*m1+b*m2+(a+b)*m3+(s1**2+s2**2+s1*s2)/(3*m) for [m1,m2,m3] in mvals]\n",
    "        overMax = np.greater_equal(exp,expMax)\n",
    "        if np.logical_and.reduce(overMax):\n",
    "            rejected+=1\n",
    "        else:\n",
    "            rejected = 0\n",
    "            [par.append(x) for x in np.array(exp,dtype=sage.rings.rational.Rational)[np.logical_not(overMax)].tolist()]\n",
    "        amax+=1\n",
    "    return sum([q^x for x in par])\n",
    "\n",
