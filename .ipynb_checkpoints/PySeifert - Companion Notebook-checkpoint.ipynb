    "class Seifert():\n",
    "    def __init__(self, SeifertData, qvar = var('q')):\n",
    "        \n",
    "        self.SeifertData = SeifertData\n",
    "        self.b = SeifertData[0]\n",
    "        self.q = [SeifertData[x] for x in range(1,len(SeifertData),2)]\n",
    "        self.p = [SeifertData[x] for x in range(2,len(SeifertData),2)] # p is the order of singular fibers\n",
    "        self.f = len(self.q)\n",
    "        assert self.f == 3 or self.f == 4, \"PySeifert only currently supports three and four fibers\"\n",
    "        self.M = self._getPlumbingMatrix()\n",
    "        self.L = self.M.ncols()\n",
    "        self.Mdet = self.M.det()\n",
    "        self.Minv = self.M.inverse()\n",
    "        self.d = 1/GCD(self.Minv[0])\n",
    "        self.m = abs(self.Minv[0,0]*self.d)\n",
    "        self.deg = self._getDeg() # returns 2-deg(v) of plumbing matrix\n",
    "        self.A = vector([x if y==1 else 0 for x,y in zip(self.Minv[0],self.deg)])\n",
    "        self.chi = self._getChi()\n",
    "        self.qvar = qvar\n",
    "        self.Coker = self._getCoker()\n",
    "        self.graph = self._getGraph()\n",
    "        self.SInRange = False #Unused\n",
    "    \n",
    "    def _ContFrac(self, x, a = -1, iterMax = 1000):\n",
    "        r\"\"\"\n",
    "        Takes a rational number x, an integer a and computes the continued function \n",
    "        with a multiplicative parameter a, up to iterMax iterations.\n",
    "        \n",
    "        Specifically:\n",
    "            x |--> [a_0,a_1,...,a_n]\n",
    "            x = a_0 + a/(a_1 + a/(a_2+...))\n",
    "            \n",
    "        Args:\n",
    "            x <class 'sage.rings.rational.Rational'>: fraction to expand\n",
    "            a <class 'sage.rings.integer.Integer'>: expansion parameter\n",
    "            expMax <class 'sage.rings.integer.Integer'>: iteration maximum\n",
    "        \n",
    "        Returns:\n",
    "            list <class 'sage.rings.integer.Integer'>: continued fraction sequence\n",
    "            \n",
    "        Most of the code is inspired by that of Cagri Karakurt and available at \n",
    "        https://share.cocalc.com/share/d1efa37e-be6a-40f6-80c7-8a34201d7c4e/PlumbGraph.sagews?viewer=share\n",
    "        as of writing (July 4th 2021)\n",
    "        \"\"\"\n",
    "        assert x in QQ, \"x must be a rational number\"\n",
    "        assert a in ZZ, \"a must be an integer\"\n",
    "        assert iterMax in NN, \"iterMax must be a positive integer\"\n",
    "        \n",
    "        n=0\n",
    "        r = list()\n",
    "        while n<iterMax:\n",
    "            r.append(ceil(x))\n",
    "            if x==ceil(x):\n",
    "                break\n",
    "            else:\n",
    "                x=1/(ceil(x)+a*x)\n",
    "                n+=1\n",
    "        if n < iterMax:\n",
    "            return r\n",
    "        else:\n",
    "            logging.warning(f\"Maximum iteration reached ({iterMax})\")\n",
    "            return r\n",
    "        \n",
    "    def _getPlumbingMatrix(self):\n",
    "        r\"\"\"\n",
    "        Compute the plumbing matrix of a given set of SeifertData w.\n",
    "        \n",
    "        Specifically:\n",
    "            w |--> M\n",
    "            \n",
    "        Args:\n",
    "            w = [b,q_1,p_1,...,q_f,p_f]\n",
    "        \n",
    "        Returns:\n",
    "            M = <class 'sage.matrix.matrix_integer_dense.Matrix_integer_dense'>\n",
    "            \n",
    "        Most of the code is inspired by that of Cagri Karakurt and available at \n",
    "        https://share.cocalc.com/share/d1efa37e-be6a-40f6-80c7-8a34201d7c4e/PlumbGraph.sagews?viewer=share\n",
    "        as of writing (July 4th 2021)\n",
    "        \"\"\"\n",
    "        logging.debug([(f\"q={self.SeifertData[2*i]}\",f\"p={self.SeifertData[2*i-1]}\") for i in range(1,self.f+1)])\n",
    "        l=[len(self._ContFrac(self.SeifertData[2*i]/self.SeifertData[2*i-1])) for i in range(1,self.f+1)]\n",
    "        M = matrix(1+sum(l))\n",
    "        M[0,0] = self.b\n",
    "        for j in range(len(l)):\n",
    "            for k in range(l[j]):\n",
    "                if k == 0:\n",
    "                    M[0,1+sum(l[:j])]=1\n",
    "                    M[1+sum(l[:j]),0]=1\n",
    "                    M[1+k+sum(l[:j]),1+k+sum(l[:j])] = (-1)*self._ContFrac(self.SeifertData[2*j+2]/self.SeifertData[2*j+1])[k]\n",
    "                else:\n",
    "                    M[1+k+sum(l[:j]),k+sum(l[:j])]=1\n",
    "                    M[k+sum(l[:j]),1+k+sum(l[:j])]=1\n",
    "                    M[1+k+sum(l[:j]),1+k+sum(l[:j])]=(-1)*self._ContFrac(self.SeifertData[2*j+2]/self.SeifertData[2*j+1])[k]\n",
    "        return M\n",
    "    \n",
    "    def _getCoker(self):\n",
    "        r\"\"\"\n",
    "        Compute the cokernel of the plumbing matrix.\n",
    "        \n",
    "        Specifically:\n",
    "            self.M |--> Coker\n",
    "            \n",
    "        Args:\n",
    "        \n",
    "        Returns:\n",
    "            Coker list()\n",
    "        \"\"\"\n",
    "        \n",
    "        if self.Mdet**2 == 1:\n",
    "            return [[0]*self.L]\n",
    "        Coker = []\n",
    "        for v in range(self.L):\n",
    "            vec = vector([0 for _ in range(self.L)])\n",
    "            for i in range(abs(self.Mdet) - 1):\n",
    "                vec[v] = i+1\n",
    "                new = [x-floor(x) for x in self.Minv*vec]\n",
    "                if new not in Coker:\n",
    "                    Coker.append(new)\n",
    "            if len(Coker) == abs(self.Mdet): break\n",
    "        return Coker\n",
    "                          \n",
    "    def _getDeg(self):\n",
    "        \"\"\"\n",
    "        Compute the degrees of the vertices in the order that they appear in the plumbing matrix.\n",
    "        Returns 2-deg(v)\n",
    "        \"\"\"\n",
    "        deg = list()\n",
    "        for i,row in enumerate(list(self.M)):\n",
    "            deg.append(2-(sum(row)-row[i]))\n",
    "        return deg\n",
    "    \n",
    "    def _getGraph(self):\n",
    "        \"\"\"\n",
    "        Construct the graph from plumbing matrix\n",
    "        \"\"\"\n",
    "        g=Graph()\n",
    "        labels = list()\n",
    "        for d in self.M.diagonal():\n",
    "            if str(d) in g:\n",
    "                n=1\n",
    "                while str(d)+\".\"+str(n) in g:\n",
    "                    n+=1\n",
    "                g.add_vertex(str(d)+\".\"+str(n))\n",
    "                labels.append(str(d)+\".\"+str(n))\n",
    "            else:\n",
    "                g.add_vertex(str(d))\n",
    "                labels.append(str(d))\n",
    "\n",
    "        for i,j in itertools.combinations(range(self.L),r=2):\n",
    "                    if self.M[i][j] == 1:\n",
    "                        g.add_edge(str(labels[i]),str(labels[j]))\n",
    "        return g\n",
    "    \n",
    "    def _getRho(self, rk):\n",
    "        assert rk == 1 or rk == 2, \"PySeifert only supports ranks 1 or 2\"\n",
    "        if rk == 1: return vector([1/2])\n",
    "        elif rk == 2: return vector([1,1])\n",
    "    \n",
    "    def _allSValsBriesk(self, rk,showTable,printOnly):\n",
    "        allSVals = []\n",
    "        WG, WL = AWeylGroup(rk,WL=True)\n",
    "        rho = self._getRho(rk)\n",
    "    \n",
    "    def _getChi(self):\n",
    "        return sum([self.SeifertData[0]]+[self.SeifertData[2*i+1]/self.SeifertData[2*i+2] for i in range(self.f)])\n",
    "        \n",
    "    \n",
    "    def bSet(self,rk):\n",
    "        \"\"\"\n",
    "        Compute the set of flat connections for the Seifert manifold with respect to A_rk group.\n",
    "        \"\"\"\n",
    "        # If Brieskorn sphere, just return the flat connection\n",
    "        if self.Mdet**2 == 1:\n",
    "            return [[vector([-(i+1)*(rk-i) for i in range(rk)])*x/2 for x in self.deg]]\n",
    "        \n",
    "        # Tensor (Coker \\otimes \\Lambda)\n",
    "        e = identity_matrix(rk)\n",
    "        Bset = list()\n",
    "        for ei,v in itertools.product(e,self.Coker):\n",
    "            Bset.append([x*ei for x in v])\n",
    "            \n",
    "        # Calculate the orbit of each connection w/r. to the Weyl group.\n",
    "        # If an orbit element is already present in Bset, then remove connection.\n",
    "        toRemove = list()\n",
    "        for i,b in enumerate(Bset):\n",
    "            for g in AWeylGroup(rk):\n",
    "                gb=[g*vector(x) for x in b]\n",
    "                for row in range(self.L):\n",
    "                    for alpha in range(rk):\n",
    "                        if gb[row][alpha] < 0: gb[row][alpha] += 1\n",
    "                if gb in Bset[:i]:\n",
    "                    toRemove.insert(0,i)\n",
    "                    break\n",
    "        for i in toRemove:\n",
    "            del Bset[i]\n",
    "        # Finally compute 2Mb-(i+1)(rk-i)deg and return the proper form of the connection\n",
    "        Bset = [list(2*self.M*matrix(b)+matrix([vector([-(i+1)*(rk-i) for i in range(rk)])*x*1/2 for x in self.deg])) for b in Bset]\n",
    "        return Bset\n",
    "    \n",
    "    def delta(self,rk):\n",
    "        return NormL(self._getRho(rk))/2*sum([self.Minv[0,i]/self.Minv[0,0]-self.Minv[i,i] for i,d in enumerate(self.deg) if d==1])\n",
    "    \n",
    "    def sSet(self, rk, wh, b):\n",
    "        SSet = []\n",
    "        rho = self._getRho(rk)\n",
    "        for v in itertools.product(range(abs(self.d)),repeat = rk):\n",
    "            whIter = iter(wh)\n",
    "            kappa = [vector(v) if d == -1 else (next(whIter)*rho if d == 1 else vector([0]*rk)) for d in self.deg]\n",
    "            if np.logical_and.reduce([row in ZZ^rk for row in self.Minv*(matrix(kappa)-matrix(b))]):\n",
    "                SSet.append(kappa[0]-rho)\n",
    "        return SSet\n",
    "\n",
    "    def sVals(self,b,showTable=False, printOnly=False):\n",
    "        #if self.Mdet**2 == 1:\n",
    "        #    return self._allSValsBriesk(rk,showTable,printOnly)\n",
    "        rk = len(b[0])\n",
    "        allSVals = []\n",
    "        WG, WL = AWeylGroup(rk,WL=True)\n",
    "        rho = self._getRho(rk)\n",
    "        for ind in tuples(range(len(WG)),self.f):\n",
    "            temp = iter(ind)\n",
    "            wh = [WG[next(temp)]*rho if d == 1 else vector([0]*rk) for d in self.deg]\n",
    "            whl = sum([WL[i] for i in ind])\n",
    "            sSet = self.sSet(rk,[WG[i] for i in ind],b)\n",
    "            if sSet != []:\n",
    "                allSVals.append([whl,self.d*CartanM(rk)*sum(matrix(self.A)*matrix(wh)),sSet,ind])\n",
    "        if showTable or printOnly: print(tabulate(allSVals, headers=[\"wl\",\"s\",\"κ\",\"Group elements\"]))\n",
    "        if not printOnly: \n",
    "            return allSVals\n",
    "    \n",
    "    def LogVOATripChi(self,s,lam,expMax, explicit = True):\n",
    "        assert lam in (vector([0,0]),vector([1,0]),vector([0,1])), \"Lambda should be (0,0), (1,0) or (0,1)\"\n",
    "        rejected = 0\n",
    "        amax = 1\n",
    "        s1, s2 = s\n",
    "        rk = 2\n",
